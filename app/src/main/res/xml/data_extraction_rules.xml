<?xml version="1.0" encoding="utf-8"?>
<!-- API 31+ 백업 규칙 -->
<data-extraction-rules>
    <cloud-backup>
        <!-- DataStore preferences 백업 제외 (lastSyncTime 복원 방지) -->
        <exclude domain="file" path="datastore/settings.preferences_pb" />
    </cloud-backup>
    <device-transfer>
        <!-- 기기 이전 시에도 DataStore 제외 -->
        <exclude domain="file" path="datastore/settings.preferences_pb" />
    </device-transfer>
</data-extraction-rules>
